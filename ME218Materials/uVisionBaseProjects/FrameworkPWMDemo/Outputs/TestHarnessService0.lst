N/*----------------------------- Module Defines ----------------------------*/
N// these times assume a 1.000mS/tick timing
N#define ONE_SEC 1000
N#define HALF_SEC (ONE_SEC/2)
N#define TWO_SEC (ONE_SEC*2)
N#define FIVE_SEC (ONE_SEC*5)
N
N/*---------------------------- Module Functions ---------------------------*/
N/* prototypes for private functions for this service.They should be functions
N   relevant to the behavior of this service
N*/
N
Nstatic void InitLED(void);
Nstatic void BlinkLED(void);
N
N/*---------------------------- Module Variables ---------------------------*/
N// with the introduction of Gen2, we need a module level Priority variable
Nstatic uint8_t MyPriority;
N// add a deferral queue for up to 3 pending deferrals +1 to allow for ovehead
Nstatic ES_Event DeferralQueue[3+1];
N
N/*------------------------------ Module Code ------------------------------*/
N/****************************************************************************
N Function
N     InitTestHarnessService0
N
N Parameters
N     uint8_t : the priorty of this service
N
N Returns
N     bool, false if error in initialization, true otherwise
N
N Description
N     Saves away the priority, and does any 
N     other required initialization for this service
N Notes
N
N Author
N     J. Edward Carryer, 01/16/12, 10:00
N****************************************************************************/
Nbool InitTestHarnessService0 ( uint8_t Priority )
X_Bool InitTestHarnessService0 ( uint8_t Priority )
N{
N  ES_Event ThisEvent;
N  uint16_t StopTime;
N  
N  MyPriority = Priority;
N  /********************************************
N   in here you write your initialization code
N   *******************************************/
N	// initialize deferral queue for testing Deferal function
N  ES_InitDeferralQueueWith( DeferralQueue, ARRAY_SIZE(DeferralQueue) );
X  ES_InitQueue( DeferralQueue, (sizeof(DeferralQueue)/sizeof(DeferralQueue[0])) );
N	// initialize LED drive for testing/debug output
N	InitLED();
N
N  // block a little bit to test if ES_GetTime() is ticking here
N  StopTime = ES_Timer_GetTime() + 3;
N  while( ES_Timer_GetTime() != StopTime )
N    putchar('.');
X    putc('.', (& __stdout));
N  // post the initial transition event
N  ThisEvent.EventType = ES_INIT;
N  if (ES_PostToService( MyPriority, ThisEvent) == true)
X  if (ES_PostToService( MyPriority, ThisEvent) == 1)
N  {
N      return true;
X      return 1;
N  }else
N  {
N      return false;
X      return 0;
N  }
N}
N
N/****************************************************************************
N Function
N     PostTestHarnessService0
N
N Parameters
N     EF_Event ThisEvent ,the event to post to the queue
N
N Returns
N     bool false if the Enqueue operation failed, true otherwise
N
N Description
N     Posts an event to this state machine's queue
N Notes
N
N Author
N     J. Edward Carryer, 10/23/11, 19:25
N****************************************************************************/
Nbool PostTestHarnessService0( ES_Event ThisEvent )
X_Bool PostTestHarnessService0( ES_Event ThisEvent )
N{
N  return ES_PostToService( MyPriority, ThisEvent);
N}
N
N/****************************************************************************
N Function
N    RunTestHarnessService0
N
N Parameters
N   ES_Event : the event to process
N
N Returns
N   ES_Event, ES_NO_EVENT if no error ES_ERROR otherwise
N
N Description
N   add your description here
N Notes
N   
N Author
N   J. Edward Carryer, 01/15/12, 15:23
N****************************************************************************/
NES_Event RunTestHarnessService0( ES_Event ThisEvent )
N{
N  ES_Event ReturnEvent;
N  ReturnEvent.EventType = ES_NO_EVENT; // assume no errors
N	static char DeferredChar = '1';
N  
N  switch (ThisEvent.EventType){
N    case ES_INIT :
N      ES_Timer_InitTimer(SERVICE0_TIMER, HALF_SEC);
X      ES_Timer_InitTimer(15, (1000/2));
N      InitPWMDemo();
N      puts("Service 00:");
N      printf("\rES_INIT received in Service %d\r\n", MyPriority);
N    
N      break;
N    case ES_TIMEOUT :  // re-start timer & announce
N      ES_Timer_InitTimer(SERVICE0_TIMER, FIVE_SEC);
X      ES_Timer_InitTimer(15, (1000*5));
N      printf("ES_TIMEOUT received from Timer %d in Service %d\r\n", 
N              ThisEvent.EventParam, MyPriority);
N      BlinkLED();
N      break;
N    case ES_NEW_KEY :  // announce
N      printf("ES_NEW_KEY received with -> %c <- in Service 0\r\n", 
N              (char)ThisEvent.EventParam);
N      if( 'd' == ThisEvent.EventParam ){
N          ThisEvent.EventParam = DeferredChar++; //
N          if (ES_DeferEvent( DeferralQueue, ThisEvent )){
X          if (ES_EnQueueLIFO( DeferralQueue, ThisEvent )){
N            puts("ES_NEW_KEY deferred in Service 0\r");
N          }
N      }
N      if( 'r' == ThisEvent.EventParam ){
N          ThisEvent.EventParam = 'Q'; // This one gets posted normally
N          ES_PostToService( MyPriority, ThisEvent);
N          // but we slide the deferred events under it so it(they) should come out first
N          if ( true == ES_RecallEvents( MyPriority, DeferralQueue )){
X          if ( 1 == ES_RecallEvents( MyPriority, DeferralQueue )){
N            puts("ES_NEW_KEY(s) recalled in Service 0\r");
N					DeferredChar = '1';
N          }
N      }
N      if( '1' == ThisEvent.EventParam ){
N          Set100_0DC();
N          puts("Set Duty Cycles to 0 & 100%\r\n");
N      }
N      break;
N    default :
N      break;
N  }
N  return ReturnEvent;
N}
