Nstatic void InitLED(void)
N{
N	volatile uint32_t Dummy;
N	// enable the clock to Port F
N	HWREG(SYSCTL_RCGCGPIO) |= SYSCTL_RCGCGPIO_R5;
X	(*((volatile uint32_t *)(0x400FE608))) |= 0x00000020;
N	// kill a few cycles to let the peripheral clock get going
N	Dummy = HWREG(SYSCTL_RCGCGPIO);
X	Dummy = (*((volatile uint32_t *)(0x400FE608)));
N	//SysCtlPeripheralEnable(SYSCTL_PERIPH_GPIOF);
N	
N	// Enable pins for digital I/O
N	HWREG(GPIO_PORTF_BASE+GPIO_O_DEN) |= (GPIO_PIN_1 | GPIO_PIN_2 | GPIO_PIN_3);
X	(*((volatile uint32_t *)(0x40025000+0x0000051C))) |= (0x00000002 | 0x00000004 | 0x00000008);
N	
N	// make pins 1,2 & 3 on Port F into outputs
N	HWREG(GPIO_PORTF_BASE+GPIO_O_DIR) |= (GPIO_PIN_1 | GPIO_PIN_2 | GPIO_PIN_3);
X	(*((volatile uint32_t *)(0x40025000+0x00000400))) |= (0x00000002 | 0x00000004 | 0x00000008);
N	//GPIOPinTypeGPIOOutput(GPIO_PORTF_BASE, GPIO_PIN_1|GPIO_PIN_2|GPIO_PIN_3);
N}
N
N
Nstatic void BlinkLED(void)
N{
N	static uint8_t LEDvalue = 2;
N	
N	// Turn off all of the LEDs
N	HWREG(GPIO_PORTF_BASE+(GPIO_O_DATA + (0xff<<2))) &= ~(GPIO_PIN_1 | GPIO_PIN_2 | GPIO_PIN_3);
X	(*((volatile uint32_t *)(0x40025000+(0x00000000 + (0xff<<2))))) &= ~(0x00000002 | 0x00000004 | 0x00000008);
N	// Turn on the new LEDs
N	HWREG(GPIO_PORTF_BASE+(GPIO_O_DATA + (0xff<<2))) |= LEDvalue;
X	(*((volatile uint32_t *)(0x40025000+(0x00000000 + (0xff<<2))))) |= LEDvalue;
N	
N	//GPIOPinWrite(GPIO_PORTF_BASE, GPIO_PIN_1|GPIO_PIN_2|GPIO_PIN_3, LEDvalue);
N	// Cycle through Red, Green and Blue LEDs
N	if (LEDvalue == 8) 
N		{LEDvalue = 2;} 
N	else 
N		{LEDvalue = LEDvalue*2;}
N
N}
